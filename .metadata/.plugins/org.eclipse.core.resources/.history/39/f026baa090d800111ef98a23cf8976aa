package ch16.ex16_04;

public class ClassAnnotation {
	public static void main (String[] args) {
		if (args.length < 1) {
			throw new IllegalArgumentException("too few parameter");
		}
		
		try {
			Class<?> cls = Class.forName(args[0]);
			
		} catch (ClassNotFoundException e) {
			e.printStackTrace();
		}
		
	}

}
